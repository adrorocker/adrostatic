{
    "name": "adrorocker/adrostatic",
    "description": "A simple Markdown based Blog",
    "license": "MIT",
    "keywords": ["md", "php", "markdown", "html", "adrorocker"],
    "homepage": "https://github.com/adrorocker/adrostatic",
    "type": "library",
    "authors": [
        {
            "name": "Adro Rocker",
            "email": "alejandro.morelos@jarwebdev.com",
            "homepage": "https://github.com/adrorocker"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "pimple/pimple": "^3.2",
        "symfony/yaml": "^3.3",
        "erusev/parsedown": "^1.6",
        "league/plates": "^3.0",
        "erusev/parsedown-extra": "^0.7.1",
        "dflydev/dot-access-data": "^1.0",
        "league/flysystem": "^1.0",
        "illuminate/support": "^5.4",
        "cocur/slugify": "^3.0",
        "slim/http": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "AdroStatic\\": "src/"
        },
        "files": [
            "src/functions.php",
            "src/dump.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AdroStatic\\Tests\\": "tests/"
        },
        "files": [
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
